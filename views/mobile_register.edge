<body class="bg-dark">

    <div class="choose_img_modal" style="width:100%;height:87%;" id="thanbel">
        <img class="modal-img" style="left:27%;" id="imagee" src="/img/70914.png" alt="avatar"/>
        <img class="modal-img" style="left:27%;" id="imagee-1" src="/img/84318.jpg" alt="avatar"/>
        <img class="modal-img" style="left:27%;" id="imagee-2" src="/img/106723.png" alt="avatar"/>
        <img class="modal-img" style="left:27%;" id="imagee-3" src="/img/188754.jpg" alt="avatar"/>
     
        <button id="btn_next_modal" style="left:50%;" class="modal_next_btn">Next</button>
    </div>
  <div class="modal-wrap" id="modal-wrap"></div>

    <div class="sufee-login d-flex align-content-center flex-wrap">
        <div class="container">
            <div class="login-content">
                <div class="login-logo">
                    <a href="index.html">
                        <h2 style="text-align: center;">Sign Up</h2>
                    </a>
                </div>
                <div class="login-form" style="border-radius: 3px;">
                    <form id="submit-forrm" action="/user/sign-up" method="POST" target="uploader_iframe">
                            <div class="form-group">
                                <label>Email address</label>
                                <input type="email" style="font-size:12px;" autocomplete="off" required="true" name="email" class="form-control" placeholder="Example@gmail.com">
                            </div>
                            <div class="form-group">
                                <label>Full Name</label>
                                <input type="text" style="font-size:12px;" autocomplete="off" required="true" name="full_name" class="form-control" maxlength="15" placeholder="E.g Elizabeth Reine">
                            </div>

                            <div class="form-group">
                                <label>Password</label>
                                <input type="password" style="font-size:12px;" autocomplete="off" required="true" name="password" class="form-control" placeholder="Secret">
                            </div>
                            
                            <div class="form-group">
                                <label style="font-size:16px;text-transform: capitalize;" id="btn-to-display-modal" class="form-control" readonly>Select Avatar..</label>
                            </div>
                            
                            <div class="form-group">
                                    <input type="text" class="form-control" style="display:none;" readonly="true" id="alert-success" value="Success!" />
                            </div>
                                <input type="hidden" value="" id="user_id" name="useer_id" />
                                <input type="hidden" id="test" value="https://ssl.gstatic.com/accounts/ui/avatar_2x.png" name="img_src" />

                                <div class="checkbox">
                                    <label style="font-size:11px;">
                                <input type="checkbox"> Remember Me
                                    </label>
                                    <label class="pull-right">
                                <a style="font-size:11px;" href="#">Forgotten Password?</a>
                                     </label>
                                </div>

                                <button style="margin-top:15px;" type="submit" class="btn btn-success btn-flat m-b-30 m-t-30">Next</button>
                              
                                <div style="margin-top:15px;" class="register-link m-t-15 text-center">
                                    <p style="font-size:13px;">Already Have an account ? <a style="color:blue;text-decoration:none;font-size:13px;" href="/login"> Sign In Here</a></p>
                                </div>
                                <iframe id="uploader_iframe" name="uploader_iframe" style="display: none;"></iframe>
                    </form>
                </div>
            </div>
        </div>
    </div>

